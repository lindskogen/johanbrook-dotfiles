# misc
alias reload='. $HOME/.profile'
alias mongo_run='mongod run --config $HOME/.applications/homebrew/Cellar/mongodb/1.2.2-x86_64/mongod.conf'
alias mysql_run='$HOME/.applications/homebrew/Cellar/mysql/5.1.43/share/mysql/mysql.server start'
#Warez
alias tMovies="scp $HOME/Downloads/*.torrent debby.zapto.org:~/Downloads/Movies/ && rm -rf $HOME/Downloads/*.torrent"
