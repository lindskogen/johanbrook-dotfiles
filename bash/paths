export PATH="$HOME/.applications:$HOME/.applications/homebrew/bin:$HOME/.applications/homebrew/include:/usr/local/bin:/usr/local/sbin:$PATH"
export MANPATH="/usr/local/man:$HOME/.applications/homebrew/share/:$MANPATH"
# export PYTHONPATH=/Library/Frameworks/Python.framework/Versions/2.6/lib/python2.6/site-packages/
if [[ -s $HOME/.rvm/scripts/rvm ]] ; then source $HOME/.rvm/scripts/rvm ; fi